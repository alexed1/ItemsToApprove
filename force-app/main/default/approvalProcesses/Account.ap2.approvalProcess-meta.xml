<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>AccountNumber</field>
        <field>Site</field>
        <field>AccountSource</field>
        <field>AnnualRevenue</field>
        <field>BillingAddress</field>
        <field>CleanStatus</field>
        <field>CreatedBy</field>
        <field>DandbCompany</field>
        <field>Jigsaw</field>
        <field>Description</field>
        <field>DunsNumber</field>
        <field>NumberOfEmployees</field>
        <field>Fax</field>
        <field>Industry</field>
        <field>LastModifiedBy</field>
        <field>NaicsCode</field>
        <field>NaicsDesc</field>
        <field>OperatingHours</field>
        <field>Ownership</field>
        <field>Parent</field>
        <field>Phone</field>
        <field>Rating</field>
        <field>ShippingAddress</field>
        <field>Sic</field>
        <field>SicDesc</field>
        <field>TickerSymbol</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>test-gmaiualy1z2c@example.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Step 1</label>
        <name>Step_1</name>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Account.BillingCity</field>
            <operation>notEqual</operation>
            <value>foobarville</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>ap2</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
